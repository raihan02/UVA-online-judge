#define min(x, y) ((x) < (y) ? (x) : (y))
m mp;



#define deb(x) cerr << #x << " = " << x << endl;

int main()
{
    int o = 0;
    int city[50][50] , ct , road , tc;
    while(cin >> ct >> road )
    {
        if(!ct and !road)
            break;
        o++;



        char s1[30][30];
          memset(city , 63 , sizeof(city));
        f(i , 0 , ct)
        {
            scanf("%s", s1[i]);

        }

        int x , y , v;

        while(road--)
        {
            scanf("%d %d %d", &x, &y, &v);
            x--, y--;
            city[x][y] = min(v, city[x][y]);
            city[y][x] = min(v, city[y][x]);
        }

        f(k , 0 , ct)
        {
            f(i , 0 , ct)
            {
                f(j , 0 , ct)
                {
                    city[i][j] = min(city[i][j], city[i][k]+city[k][j]);
                }
            }
        }
       int ans = 0xfffffff, idx = 0, tmp;
        for(int i = 0; i < ct; i++) {
            tmp = 0;
            for(int j = 0; j < ct; j++)
                if(i != j)
                    tmp += city[i][j];
            if(tmp < ans)
                ans =  tmp, idx = i;
        }
        printf("Case #%d : %s\n", o, s1[idx]);

    }
}
