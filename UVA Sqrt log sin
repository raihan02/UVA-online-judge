int a , b;
int sum (int a)
{
    int x = 0;

    for(int i = 1; i <= a; i++)
        x += i;
    return x;
}
int x[1000005];
int main()
{
   x[0] = 1;
   for(int i = 1; i <= 1000000; i++)
   {
        x[i] = x[(int)floor(i - sqrt((double)i))]
               + x[(int)floor(log((double)i))]
               + x[(int)floor(i * sin((double)i) * sin((double)i))];
        x[i] %= 1000000;
   }
   int n;

   while(cin >> n  and n != -1)
   {
     printf("%d\n",x[n]);
   }

}
